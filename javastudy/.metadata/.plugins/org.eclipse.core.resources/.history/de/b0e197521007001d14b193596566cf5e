package ex03_number;

public class Ex01 {

	public static void main(String[] args) {
		
		// 산술 연산
		int a = 7;
		int b = 2;
		int result1 = a + b;
		int result2 = a - b;
		int result3 = a * b;
		int result4 = a / b;	// 몫
		int result5 = a % b;	// 나머지
		
		System.out.ptintln(result1);
		System.out.ptintln(result2);
		System.out.ptintln(result3);
		System.out.ptintln(result4);
		System.out.ptintln(result5);
		
	}

}
